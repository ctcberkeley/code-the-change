<h1>Officers</h1>
 
<%= link_to 'New officer', new_officer_path %>

<table>
  <tr>
    <th>Name</th>
    <th>Position</th>
    <th>Image URL</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
  <% @officers.each do |officer| %>
    <tr>
      <td><%= officer.name %></td>
      <td><%= officer.position %></td>
      <td><%= officer.img_url %></td>
      <td><%= link_to 'Show', officer %></td>
      <td><%= link_to 'Edit', edit_officer_path(officer) %></td>
      <td><%= link_to 'Destroy', officer_path(officer), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>